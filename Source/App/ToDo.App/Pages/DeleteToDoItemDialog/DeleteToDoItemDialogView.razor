<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">@Dialog.Title</MudText>
    </TitleContent>
    <DialogActions>
        <MudButton Color="Color.Error" OnClick="Dialog.Cancel">Cancel</MudButton>
        <MudSpacer/>
        <MudButton Color="Color.Primary" OnClick="DeleteToDoItem">Save</MudButton>
    </DialogActions>
</MudDialog>


@code{

    [CascadingParameter]
    MudDialogInstance Dialog { get; set; } = null!;

    [Parameter]
    public Guid ToDoItemId { get; set; }

    private void DeleteToDoItem()
    {
        Dialog.Close(DialogResult.Ok(ToDoItemId));
    }
}